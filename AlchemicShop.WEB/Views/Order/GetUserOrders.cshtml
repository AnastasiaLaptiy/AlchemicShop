@model IEnumerable<AlchemicShop.WEB.Models.OrderViewModel>
<table class="table">
    <tr>
        <th>
            Order
        </th>
        <th>
            User
        </th>
        <th>
            Sheduled Date
        </th>
        <th>
            Closed Date
        </th>
        <th>
            Status
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            @foreach (var user in ViewBag.GetUserName)
            {
                if (user.Id == item.UserId)
                {
                    <td>
                        @user.Name
                    </td>
                }
            }
            <td>
                @Html.DisplayFor(modelItem => item.SheduledDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClosedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.ActionLink("Details", "GetOrderDetails", new { id = item.Id })
            </td>
        </tr>
    }
</table>
}


